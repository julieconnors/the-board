<h1>Register</h1>
<%= form_for @user do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :email %>
<%= f.text_field :email %><br>
<%= f.label :password %>
<%= f.password_field :password %><br>
<%= f.label :trainer %>
<%= f.check_box :trainer %><br>
<%= f.submit "Register"%>
<% end %>